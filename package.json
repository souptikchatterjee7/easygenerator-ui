{
    "name": "easygenerator-ui",
    "version": "1.00.00",
    "private": true,
    "dependencies": {
        "@reduxjs/toolkit": "^2.2.5",
        "@testing-library/jest-dom": "^5.17.0",
        "@testing-library/react": "^13.4.0",
        "@testing-library/user-event": "^13.5.0",
        "axios": "^1.7.2",
        "prettier": "^3.3.2",
        "pretty-quick": "^4.0.0",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "react-redux": "^9.1.2",
        "react-router-dom": "^6.23.1",
        "react-scripts": "5.0.1",
        "web-vitals": "^2.1.4"
    },
    "scripts": {
        "start": "react-scripts start",
        "start:stg": "react-scripts start stg",
        "start:prod": "react-scripts start prod",
        "prepare": "husky install .husky",
        "lint": "npx eslint src/",
        "pretty": "npx pretty-quick --staged",
        "prettify": "prettier --write .",
        "lintify": "npm run lint -- --fix",
        "fix": "npm run prettify && npm run lintify",
        "pre-commit": "npm run pretty && npm run lint"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
        "@eslint/js": "^9.5.0",
        "@typescript-eslint/parser": "^7.13.0",
        "eslint": "^8.57.0",
        "eslint-plugin-react": "^7.34.2",
        "globals": "^15.4.0",
        "husky": "^9.0.11",
        "typescript-eslint": "^7.13.0"
    },
    "husky": {
        "hooks": {
            "pre-commit": "npm run pre-commit"
        }
    }
}
